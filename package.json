{
  "name": "prompt-actions",
  "description": "Action manager for prompt-base.",
  "version": "2.0.0",
  "homepage": "https://github.com/enquirer/prompt-actions",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "enquirer/prompt-actions",
  "bugs": {
    "url": "https://github.com/enquirer/prompt-actions/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "mocha"
  },
  "keywords": [
    "actions",
    "prompt"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "enquirer",
        "prompt-base",
        "prompt-choices"
      ]
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "base-prompt",
      "prompt-base"
    ]
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-format-md": "^0.1.12",
    "gulp-istanbul": "^1.1.1",
    "gulp-mocha": "^3.0.1",
    "is-windows": "^1.0.1",
    "mocha": "^3.4.1",
    "prompt-choices": "^3.0.2"
  }
}
